pipeline {
    agent any 
        stages {
            stage("build") {
                steps {
                   
                    sh "this is build"
                    
                }
            }
              stage("Test") {
                steps {
                 
                    sh "this is test"
                   
                }
            }
              stage("deploy") {
                steps {
                   
                    sh "this is deploy"
                    
                }
            }
        }
    
}